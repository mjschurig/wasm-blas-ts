{
  "name": "wasm-blas-ts",
  "version": "0.1.0",
  "description": "WebAssembly implementation of BLAS for TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:wasm": "mkdir -p build && cd build && emcmake cmake .. && emmake make",
    "build:ts": "tsup src/index.ts --format cjs,esm --dts --clean",
    "copy:wasm": "mkdir -p dist && cp build/blas.js build/blas.wasm dist/",
    "build": "npm run build:wasm && npm run build:ts && npm run copy:wasm",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint '{src,examples,tests}/**/*.ts' --max-warnings=0",
    "lint:fix": "eslint '{src,examples,tests}/**/*.ts' --fix",
    "format": "prettier --write '{src,examples,tests}/**/*.{ts,js,json}'",
    "format:check": "prettier --check '{src,examples,tests}/**/*.{ts,js,json}'",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "npm run build && npm run test && npm run lint"
  },
  "keywords": [
    "blas",
    "webassembly",
    "wasm",
    "typescript",
    "linear-algebra",
    "numerical-computing",
    "high-performance"
  ],
  "author": "Max Schurig",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxschurig/wasm-blas-ts.git"
  },
  "bugs": {
    "url": "https://github.com/maxschurig/wasm-blas-ts/issues"
  },
  "homepage": "https://github.com/maxschurig/wasm-blas-ts#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
